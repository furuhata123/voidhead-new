---
pagination:
  data: collections
  size: 1
  alias: tag
  filter: tagList
permalink: "/tags/{{ tag | slug }}/"
layout: layout.njk
---

<h1>Tag: {{ tag }}</h1>

<ul>
  {% for post in collections[tag] %}
    <li>
      <a href="{{ post.url }}">{{ post.data.title }}</a>
    </li>
  {% endfor %}
</ul>
